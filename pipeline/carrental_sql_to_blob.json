{
	"name": "carrental_sql_to_blob",
	"properties": {
		"activities": [
			{
				"name": "Copy_sql",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Carrental_sql_DS",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Carrental_sink_DS",
						"type": "DatasetReference",
						"parameters": {
							"folder": "reservations",
							"file_name": "reservations.csv"
						}
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2025-09-07T16:05:24Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}